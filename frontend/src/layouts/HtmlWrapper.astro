---
import BaseHead from "../components/BaseHead.astro";
import type { Props as BaseHeadProps } from '../components/BaseHead.astro';

export interface Props {
	seo?: BaseHeadProps;
}

const { seo } = Astro.props as Props;

---


<html lang="en">
	<head>
		<BaseHead {...seo} />
		<slot name="head" />
	</head>
	<body>
		<slot name="header" />
		<main>
			<slot />
		</main>
		<slot name="footer" />
	</body>
</html>
